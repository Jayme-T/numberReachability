function findSolution (wanted) {
    function find (start, history) {
        if (start == wanted)
            return history;
        else if (start > wanted)
            return null;
        else 
            return find (start +5, "(" + history + " + 5)" ) ||
                    find (start * 3, "(" + history +" * 3)");
                    
    }
   return find (1, "1");
}

console.log(findSolution(11));
